{
    "basedir": "src/style/",
    "file_ext": ".styl",
    "import_rule": "@import",
    "import_with_ext": false,
    "append_to_buildfile": false,
    "allow_buildfile_outside_basedir": false,
    "use_semicolons": false,
    "quote": "'",
    "indent": "  ",
    "newline": "\n",
    "ensure_trailing_newline": true,
    "file_write_options": "utf8",
    "virtual_component_prefix": "_",
    "files": {
        ".": {
            "filename": "{{NAME}}",
            "imports": ["_"],
            "import_extend_source": true,
            "content": "{{NEWLINE}}.{{NAME}}{{NEWLINE}}{{INDENT}}// your code here",
            "no_virtual": "content"
        },
        "c": {
            "filename": "{{NAME}}__constants",
            "imports": [],
            "import_extend_source": true,
            "content": null
        },
        "m": {
            "filename": "{{NAME}}__mixins",
            "imports": ["c"],
            "import_extend_source": true,
            "content": null
        },
        "_": {
            "filename": "__",
            "imports": ["c", "m"],
            "import_extend_source": false,
            "content": "{{IMPORT}} {{QUOTE}}../_base/__{{QUOTE}}",
            "no_virtual": ["content"]
        },
        "i": {
            "filename": "index",
            "imports": ["_", "."],
            "import_extend_source": false,
            "content": null
        }
    }
}
